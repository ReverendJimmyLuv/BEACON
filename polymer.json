{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/beacon-find.html",
    "src/beacon-befound.html",
    "src/beacon-savedbeacons.html",
    "src/beacon-404.html",
    "src/beacon-settings.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
